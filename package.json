{
    "name": "parallels-desktop",
    "displayName": "parallels desktop",
    "description": "Manage Parallels Desktop virtual appliances",
    "version": "0.0.3",
    "engines": {
        "vscode": "^1.77.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onView:nodeDependencies"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "configuration": {
            "title": "Parallels",
            "properties": {
                "parallels-desktop.output_path": {
                    "type": "string",
                    "default": "/Users/cjlapao/parallels_extension",
                    "description": "Output path for Packer images"
                },
                "parallels-desktop.path": {
                    "type": "string",
                    "default": "/usr/local/bin/prlctl",
                    "description": "Path to prlctl executable"
                },
                "parallels-desktop.hashicorp.vagrant.path": {
                    "type": "string",
                    "default": "",
                    "description": "Path to vagrant executable"
                },
                "parallels-desktop.hashicorp.packer.path": {
                    "type": "string",
                    "default": "",
                    "description": "Path to packer executable"
                },
                "parallels-desktop.refreshInterval": {
                    "type": "number",
                    "default": 5000,
                    "description": "Refresh interval in milliseconds"
                }
            }
        },
        "commands": [
            {
                "command": "parallels-desktop.test",
                "title": "Parallels: Test",
                "icon": "$(refresh)",
                "category": "Parallels"
            },
            {
                "command": "parallels-desktop.test",
                "title": "Parallels: Tester",
                "icon": {
                    "light": "img/light/play.svg",
                    "dark": "img/dark/play.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.add-vm",
                "title": "Parallels: Add VM",
                "shortTitle": "Add VM",
                "icon": {
                    "light": "img/light/plus.svg",
                    "dark": "img/dark/plus.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.start-vm",
                "title": "Parallels: Start VM",
                "icon": {
                    "light": "img/light/play.svg",
                    "dark": "img/dark/play.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.start-headless-vm",
                "title": "Parallels: Start Headless VM",
                "icon": {
                    "light": "img/light/play.svg",
                    "dark": "img/dark/play.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.stop-vm",
                "title": "Stop VM",
                "category": "Parallels",
                "icon": {
                    "light": "img/light/stop.svg",
                    "dark": "img/dark/stop.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.resume-vm",
                "title": "Parallels: Resume VM",
                "shortTitle": "Resume VM",
                "icon": {
                    "light": "img/light/play.svg",
                    "dark": "img/dark/play.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.pause-vm",
                "title": "Parallels: Pause VM",
                "shortTitle": "Pause VM",
                "icon": {
                    "light": "img/light/pause.svg",
                    "dark": "img/dark/pause.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.refresh-vms",
                "title": "Parallels: Refresh",
                "icon": "$(refresh)"
            },
            {
                "command": "parallels-desktop.get-details",
                "title": "Parallels: Get Details",
                "icon": "$(refresh)",
                "category": "Parallels",
                "when": "viewItem ~= /workflow"
            },
            {
                "command": "parallels-desktop.tree-view.tree-view.remove-group",
                "title": "Remove Group",
                "shortTitle": "Remove Group",
                "icon": "$(notebook-delete-cell)"
            },
            {
                "command": "parallels-desktop.tree-view.add-group",
                "title": "Add Group"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "parallels-desktop",
                    "title": "Parallels Desktop",
                    "icon": "media/parallels_logo.svg"
                },
                {
                    "id": "parallels-desktop",
                    "title": "Parallels Desktop1",
                    "icon": "media/parallels_logo.svg"
                }
            ]
        },
        "menus": {
            "commandPalette": [
                {
                    "command": "parallels-desktop.tree-view.start-vm",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.tree-view.start-headless-vm",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.tree-view.stop-vm",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.tree-view.resume-vm",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.tree-view.pause-vm",
                    "when": "false"
                }
            ],
            "view/title": [
                {
                    "command": "parallels-desktop.tree-view.refresh-vms",
                    "when": "view == parallels-desktop && parallels-desktop:initialized",
                    "group": "navigation"
                },
                {
                    "command": "parallels-desktop.tree-view.add-vm",
                    "when": "view == parallels-desktop && parallels-desktop:hasVirtualMachines",
                    "group": "navigation"
                },
                {
                    "command": "parallels-desktop.tree-view.add-group",
                    "when": "view == parallels-desktop && parallels-desktop:initialized"
                }
            ],
            "view/item/context": [
                {
                    "command": "parallels-desktop.tree-view.start-vm",
                    "when": "view == parallels-desktop && viewItem == vm.stopped",
                    "group": "inline",
                    "order": "1"
                },
                {
                    "command": "parallels-desktop.tree-view.start-headless-vm",
                    "when": "view == parallels-desktop && viewItem == vm.stopped",
                    "order": "1"
                },
                {
                    "command": "parallels-desktop.tree-view.stop-vm",
                    "when": "view == parallels-desktop && (viewItem == vm.running)",
                    "group": "inline"
                },
                {
                    "command": "parallels-desktop.tree-view.resume-vm",
                    "when": "view == parallels-desktop && (viewItem == vm.paused || viewItem == vm.suspended)",
                    "group": "inline"
                },
                {
                    "command": "parallels-desktop.tree-view.pause-vm",
                    "when": "view == parallels-desktop && viewItem == vm.running",
                    "group": "inline"
                },
                {
                    "command": "parallels-desktop.tree-view.tree-view.remove-group",
                    "when": "view == parallels-desktop && viewItem == group",
                    "group": "inline"
                }
            ]
        },
        "views": {
            "parallels-desktop": [
                {
                    "id": "parallels-desktop",
                    "name": "Parallels Desktop",
                    "icon": "media/parallels_logo.svg",
                    "contextualTitle": "Parallels Desktop"
                }
            ]
        },
        "viewsWelcome": [
            {
                "view": "parallels-desktop1",
                "when": "parallels-desktop:initialized",
                "contents": "Welcome to Parallels Desktop1"
            },
            {
                "view": "parallels-desktop1",
                "when": "parallels-desktop:initialized && parallels-desktop:parallels == false",
                "contents": "Welcome to Parallels Desktop"
            }
        ]
    },
    "scripts": {
        "clean": "rimraf ./dist ./out",
        "vscode:prepublish": "npm run build",
        "compile": "webpack",
        "watch": "webpack --watch",
        "build": "webpack --mode production",
        "package": "webpack --mode production --devtool hidden-source-map",
        "open-in-browser": "vscode-test-web --extensionDevelopmentPath=src .",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "lint": "eslint src --ext ts",
        "lint-fix": "eslint . --ext .ts --fix",
        "format": "prettier --write \"**/*.ts\"",
        "format-check": "prettier --check \"**/*.ts\"",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "16.x",
        "@types/uuid": "^9.0.1",
        "@types/vscode": "^1.77.0",
        "@typescript-eslint/eslint-plugin": "^5.59.8",
        "@typescript-eslint/parser": "^5.59.8",
        "@vscode/test-electron": "^2.3.0",
        "@vscode/test-web": "*",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "prettier": "^2.8.3",
        "rimraf": "^3.0.1",
        "ts-loader": "^9.4.2",
        "typescript": "^4.9.5",
        "webpack": "^5.76.3",
        "webpack-cli": "^5.0.1"
    },
    "dependencies": {
        "uuid": "^9.0.0"
    }
}