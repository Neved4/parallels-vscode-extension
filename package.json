        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "glob": "^8.1.0",
        "gulp": "^4.0.2",
        "gulp-cli": "^2.3.0",
        "mocha": "^10.2.0",
