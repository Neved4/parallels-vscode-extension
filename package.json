{
    "name": "parallels-desktop",
    "displayName": "Parallels Desktop",
    "publisher": "ParallelsDesktop",
    "description": "Manage Parallels Desktop virtual appliances",
    "repository": {
        "type": "git",
        "url": "https://github.com/Parallels/parallels-vscode-extension"
    },
    "icon": "img/logo/parallels_logo.png",
    "version": "0.5.8",
    "engines": {
        "vscode": "^1.77.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "badges": [
        {
            "url": "https://github.com/Parallels/parallels-vscode-extension/actions/workflows/build.yml/badge.svg?branch=main",
            "href": "https://github.com/Parallels/parallels-vscode-extension/actions/workflows/build.yml",
            "description": "Build Status"
        }
    ],
    "preview": true,
    "keywords": [
        "parallels",
        "virtualization",
        "vagrant",
        "packer",
        "prlctl"
    ],
    "homepage": "https://parallels.com",
    "activationEvents": [
        "onView:nodeDependencies"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "configuration": {
            "title": "Parallels Desktop",
            "properties": {
                "parallels-desktop.output_path": {
                    "type": "string",
                    "default": "/Users/cjlapao/parallels_extension",
                    "description": "Output path for Packer images"
                },
                "parallels-desktop.prlctl.path": {
                    "type": "string",
                    "default": "/usr/local/bin/prlctl",
                    "description": "Path to prlctl executable"
                },
                "parallels-desktop.extension.path": {
                    "type": "string",
                    "default": "",
                    "description": "Path for the Parallels Desktop extension"
                },
                "parallels-desktop.extension.download.path": {
                    "type": "string",
                    "default": "",
                    "description": "Download cache folder for the VMs ISO's"
                },
                "parallels-desktop.hashicorp.vagrant.boxes.path": {
                    "type": "string",
                    "default": "",
                    "description": "Vagrant box files folder for new VMs"
                },
                "parallels-desktop.hashicorp.vagrant.path": {
                    "type": "string",
                    "default": "",
                    "description": "Path to vagrant executable"
                },
                "parallels-desktop.hashicorp.packer.path": {
                    "type": "string",
                    "default": "",
                    "description": "Path to packer executable"
                },
                "parallels-desktop.extension.refresh.auto": {
                    "type": "boolean",
                    "default": true,
                    "description": "Set Auto Refresh for Parallels Desktop Tree View"
                },
                "parallels-desktop.extension.refresh.interval": {
                    "type": "number",
                    "default": 60000,
                    "description": "Auto Refresh interval in milliseconds"
                },
                "parallels-desktop.tree.show-hidden-items": {
                    "type": "boolean",
                    "default": false,
                    "description": "Shows Hidden items in the tree"
                },
                "parallels-desktop.tree.order-items-alphabetically": {
                    "type": "boolean",
                    "default": true,
                    "description": "Order Groups and VMs alphabetically"
                },
                "parallels-desktop.tree.show-flat-snapshot-tree": {
                    "type": "boolean",
                    "default": false,
                    "description": "Show snapshots as a flat list"
                }
            }
        },
        "commands": [
            {
                "command": "parallels-desktop.tree-view.add-vm",
                "title": "Parallels Desktop: Add VM",
                "shortTitle": "Add VM",
                "icon": {
                    "light": "img/light/plus.svg",
                    "dark": "img/dark/plus.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.start-vm",
                "title": "Start VM",
                "icon": {
                    "light": "img/light/start.svg",
                    "dark": "img/dark/start.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.start-headless-vm",
                "title": "Start Headless VM",
                "icon": {
                    "light": "img/light/start.svg",
                    "dark": "img/dark/start.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.stop-vm",
                "title": "Stop VM",
                "category": "Parallels",
                "icon":  {
                    "light": "img/light/stop.svg",
                    "dark": "img/dark/stop.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.resume-vm",
                "title": "Parallels: Resume VM",
                "shortTitle": "Resume VM",
                "icon":  {
                    "light": "img/light/resume.svg",
                    "dark": "img/dark/resume.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.pause-vm",
                "title": "Parallels: Pause VM",
                "shortTitle": "Pause VM",
                "icon": {
                    "light": "img/light/pause.svg",
                    "dark": "img/dark/pause.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.suspend-vm",
                "title": "Suspend VM",
                "icon": {
                    "light": "img/light/suspend.svg",
                    "dark": "img/dark/suspend.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.remove-vm",
                "title": "Remove Virtual Machine",
                "icon": {
                    "light": "img/light/trash.svg",
                    "dark": "img/dark/trash.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.enter-vm",
                "title": "Get inside VM",
                "icon": {
                    "light": "img/light/terminal.svg",
                    "dark": "img/dark/terminal.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.refresh-vms",
                "title": "Parallels: Refresh",
                "icon": {
                    "light": "img/light/refresh.svg",
                    "dark": "img/dark/refresh.svg"
                }
            },
            {
                "command": "parallels-desktop.get-details",
                "title": "Parallels: Get Details",
                "category": "Parallels",
                "when": "viewItem ~= /workflow",
                "icon": {
                    "light": "img/light/info.svg",
                    "dark": "img/dark/info.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.add-group",
                "title": "Add Group",
                "icon": {
                    "light": "img/light/plus.svg",
                    "dark": "img/dark/plus.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.take-vm-snapshot",
                "title": "Take VM Snapshot",
                "category": "Parallels",
                "icon": {
                    "light": "img/light/add_snapshot.svg",
                    "dark": "img/dark/add_snapshot.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.delete-vm-snapshot",
                "title": "Delete VM Snapshot",
                "category": "Parallels",
                "icon": {
                    "light": "img/light/trash.svg",
                    "dark": "img/dark/trash.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.restore-vm-snapshot",
                "title": "Restore VM Snapshot",
                "category": "Parallels",
                "icon": {
                    "light": "img/light/restore.svg",
                    "dark": "img/dark/restore.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.view-vm-details",
                "title": "View VM details",
                "category": "Parallels",
                "icon": {
                    "light": "img/light/info.svg",
                    "dark": "img/dark/info.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.take-group-snapshot",
                "title": "Take Group Snapshot",
                "category": "Parallels",
                "icon": {
                    "light": "img/light/add_snapshot.svg",
                    "dark": "img/dark/add_snapshot.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.remove-group",
                "title": "Delete Group",
                "shortTitle": "Delete Group",
                "icon":  {
                    "light": "img/light/trash.svg",
                    "dark": "img/dark/trash.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.rename-group",
                "title": "Parallels Desktop: Rename Group",
                "shortTitle": "Rename Group",
                "icon": {
                    "light": "img/light/edit.svg",
                    "dark": "img/dark/edit.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.start-group-vms",
                "title": "Start VMs",
                "category": "Parallels",
                "icon": {
                    "light": "img/light/start.svg",
                    "dark": "img/dark/start.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.stop-group-vms",
                "title": "Stop VMs",
                "category": "Parallels",
                "icon": "$(debug-stop)"
            },
            {
                "command": "parallels-desktop.tree-view.resume-group-vms",
                "title": "Resume VMs",
                "category": "Parallels",
                "icon": "$(debug-start)"
            },
            {
                "command": "parallels-desktop.tree-view.pause-group-vms",
                "title": "Pause VMs",
                "category": "Parallels",
                "icon": "$(debug-pause)"
            },
            {
                "command": "parallels-desktop.tree-view.suspend-group-vms",
                "title": "Suspend VMs",
                "category": "Parallels",
                "icon": {
                    "light": "img/light/suspend.svg",
                    "dark": "img/dark/suspend.svg"
                }
            },
            {
                "command": "parallels-desktop.clear-download-cache",
                "title": "Clear cached downloads",
                "category": "Parallels",
                "icon": {
                    "light": "img/light/trash.svg",
                    "dark": "img/dark/trash.svg"
                }
            },
            {
                "command": "parallels-desktop.vagrant.box.provider.refresh",
                "title": "Vagrant: Refresh",
                "icon":  {
                    "light": "img/light/refresh.svg",
                    "dark": "img/dark/refresh.svg"
                }
            },
            {
                "command": "parallels-desktop.vagrant.box.provider.init",
                "title": "Vagrant: Up",
                "icon": {
                    "light": "img/light/vagrant_init.svg",
                    "dark": "img/dark/vagrant_init.svg"
                }
            },
            {
                "command": "parallels-desktop.vagrant.box.provider.delete",
                "title": "Vagrant: Delete",
                "icon":  {
                    "light": "img/light/trash.svg",
                    "dark": "img/dark/trash.svg"
                }
            },
            {
                "command": "parallels-desktop.core.enable-show-hidden-items",
                "title": "Parallels Desktop: Show hidden items",
                "icon": {
                    "light": "img/light/hide.svg",
                    "dark": "img/dark/hide.svg"
                }
            },
            {
                "command": "parallels-desktop.core.disable-show-hidden-items",
                "title": "Parallels Desktop: Hide hidden items",
                "icon": {
                    "light": "img/light/show.svg",
                    "dark": "img/dark/show.svg"
                }
            },
            {
                "command": "parallels-desktop.core.toggle-show-hidden-items",
                "title": "Parallels Desktop: Toggle items visibility",
                "icon": {
                    "light": "img/light/show.svg",
                    "dark": "img/dark/show.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.show-item",
                "title": "Show Item",
                "icon": {
                    "light": "img/light/show.svg",
                    "dark": "img/dark/show.svg"
                }
            },
            {
                "command": "parallels-desktop.tree-view.hide-item",
                "title": "Hide Item",
                "icon": {
                    "light": "img/light/hide.svg",
                    "dark": "img/dark/hide.svg"
                }
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "parallels-desktop",
                    "title": "Parallels Desktop",
                    "icon": "img/logo/parallels_logo.svg"
                }
            ]
        },
        "menus": {
            "commandPalette": [
                {
                    "command": "parallels-desktop.tree-view.start-vm",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.tree-view.start-headless-vm",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.tree-view.stop-vm",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.tree-view.resume-vm",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.tree-view.pause-vm",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.tree-view.suspend-vm",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.tree-view.enter-vm",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.tree-view.remove-vm",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.get-details",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.tree-view.take-vm-snapshot",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.tree-view.delete-vm-snapshot",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.tree-view.restore-vm-snapshot",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.tree-view.view-vm-details",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.tree-view.take-group-snapshot",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.tree-view.remove-group",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.tree-view.rename-group",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.tree-view.start-group-vms",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.tree-view.stop-group-vms",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.tree-view.resume-group-vms",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.tree-view.suspend-group-vms",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.tree-view.pause-group-vms",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.vagrant.box.provider.init",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.vagrant.box.provider.delete",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.core.enable-show-hidden-items",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.core.disable-show-hidden-items",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.tree-view.show-item",
                    "when": "false"
                },
                {
                    "command": "parallels-desktop.tree-view.hide-item",
                    "when": "false"
                }
            ],
            "view/title": [
                {
                    "command": "parallels-desktop.tree-view.refresh-vms",
                    "when": "view == parallels-desktop-machines && parallels-desktop:initialized",
                    "group": "navigation@1"
                },
                {
                    "command": "parallels-desktop.tree-view.add-vm",
                    "when": "view == parallels-desktop-machines && parallels-desktop:initialized",
                    "group": "navigation@0"
                },
                {
                    "command": "parallels-desktop.core.enable-show-hidden-items",
                    "when": "view == parallels-desktop-machines && parallels-desktop:disableShowHidden",
                    "group": "navigation@2"
                },
                {
                    "command": "parallels-desktop.core.disable-show-hidden-items",
                    "when": "view == parallels-desktop-machines && parallels-desktop:enableShowHidden",
                    "group": "navigation@2"
                },
                {
                    "command": "parallels-desktop.tree-view.add-group",
                    "when": "view == parallels-desktop-machines && parallels-desktop:initialized"
                },
                {
                    "command": "parallels-desktop.vagrant.box.provider.refresh",
                    "when": "view == parallels-desktop-machines-vagrant && parallels-desktop:initialized",
                    "group": "navigation@0"
                },
                {
                    "command": "parallels-desktop.vagrant.box.provider.refresh",
                    "when": "view == parallels-desktop-vagrant && parallels-desktop:initialized",
                    "group": "navigation@1"
                }
            ],
            "view/item/context": [
                {
                    "command": "parallels-desktop.tree-view.take-vm-snapshot",
                    "when": "view == parallels-desktop-machines && (viewItem == vm.visible.paused || viewItem == vm.visible.suspended || viewItem == vm.visible.running || viewItem == vm.visible.stopped ||viewItem == vm.hidden.paused || viewItem == vm.hidden.suspended || viewItem == vm.hidden.running || viewItem == vm.hidden.stopped)",
                    "group": "inline@19"
                },
                {
                    "command": "parallels-desktop.tree-view.view-vm-details",
                    "when": "view == parallels-desktop-machines && (viewItem == vm.visible.paused || viewItem == vm.visible.suspended || viewItem == vm.visible.running || viewItem == vm.visible.stopped || viewItem == vm.hidden.paused || viewItem == vm.hidden.suspended || viewItem == vm.hidden.running || viewItem == vm.hidden.stopped)",
                    "group": "inline@20"
                },
                {
                    "command": "parallels-desktop.tree-view.start-vm",
                    "when": "view == parallels-desktop-machines && (viewItem == vm.visible.stopped || viewItem == vm.hidden.stopped)",
                    "group": "inline@0"
                },
                {
                    "command": "parallels-desktop.tree-view.start-headless-vm",
                    "when": "view == parallels-desktop-machines && (viewItem == vm.visible.stopped || viewItem == vm.hidden.stopped)"
                },
                {
                    "command": "parallels-desktop.tree-view.enter-vm",
                    "when": "view == parallels-desktop-machines && (viewItem == vm.visible.running || viewItem == vm.hidden.running)",
                    "group": "inline@21"
                },
                {
                    "command": "parallels-desktop.tree-view.remove-vm",
                    "when": "view == parallels-desktop-machines && (viewItem == vm.visible.invalid || viewItem == vm.visible.stopped || viewItem == v.hidden.invalid || viewItem == vm.hidden.stopped)",
                    "group": "inline@22"
                },
                {
                    "command": "parallels-desktop.tree-view.stop-vm",
                    "when": "view == parallels-desktop-machines && (viewItem == vm.visible.running || viewItem == vm.hidden.running)",
                    "group": "inline"
                },
                {
                    "command": "parallels-desktop.tree-view.resume-vm",
                    "when": "view == parallels-desktop-machines && (viewItem == vm.visible.paused || viewItem == vm.visible.suspended || viewItem == vm.hidden.paused || viewItem == vm.hidden.suspended)",
                    "group": "inline"
                },
                {
                    "command": "parallels-desktop.tree-view.pause-vm",
                    "when": "view == parallels-desktop-machines && (viewItem == vm.visible.running || viewItem == vm.hidden.running)",
                    "group": "inline"
                },
                {
                    "command": "parallels-desktop.tree-view.suspend-vm",
                    "when": "view == parallels-desktop-machines && (viewItem == vm.visible.running || viewItem == vm.hidden.running)",
                    "group": "inline"
                },
                {
                    "command": "parallels-desktop.tree-view.delete-vm-snapshot",
                    "when": "view == parallels-desktop-machines && (viewItem == snapshot.other || viewItem == snapshot.current)",
                    "group": "inline"
                },
                {
                    "command": "parallels-desktop.tree-view.restore-vm-snapshot",
                    "when": "view == parallels-desktop-machines && viewItem == snapshot.other",
                    "group": "inline@1"
                },
                {
                    "command": "parallels-desktop.tree-view.remove-group",
                    "when": "view == parallels-desktop-machines && (viewItem == group.hidden.unknown || viewItem == group.hidden.stopped || viewItem == group.hidden.mixed || viewItem == group.hidden.running || viewItem == group.hidden.paused || viewItem == group.hidden.suspended || viewItem == group.visible.unknown || viewItem == group.visible.stopped || viewItem == group.visible.mixed || viewItem == group.visible.running || viewItem == group.visible.paused || viewItem == group.visible.suspended)",
                    "group": "inline@24"
                },
                {
                    "command": "parallels-desktop.tree-view.show-item",
                    "when": "view == parallels-desktop-machines && (viewItem == vm.hidden.invalid || viewItem == vm.hidden.stopped || viewItem == vm.hidden.running ||viewItem == vm.hidden.paused || viewItem == vm.hidden.suspended || viewItem == group.hidden.unknown || viewItem == group.hidden.stopped || viewItem == group.hidden.mixed || viewItem == group.hidden.running || viewItem == group.hidden.paused || viewItem == group.hidden.suspended)",
                    "group": "inline@25"
                },
                {
                    "command": "parallels-desktop.tree-view.hide-item",
                    "when": "view == parallels-desktop-machines && (viewItem == vm.visible.invalid || viewItem == vm.visible.stopped || viewItem == vm.visible.running ||viewItem == vm.visible.paused || viewItem == vm.visible.suspended || viewItem == group.visible.unknown || viewItem == group.visible.stopped || viewItem == group.visible.mixed || viewItem == group.visible.running || viewItem == group.visible.paused || viewItem == group.visible.suspended)",
                    "group": "inline@25"
                },
                {
                    "command": "parallels-desktop.tree-view.rename-group",
                    "when": "view == parallels-desktop-machines && (viewItem == group.hidden.unknown || viewItem == group.hidden.stopped || viewItem == group.hidden.mixed || viewItem == group.hidden.running || viewItem == group.hidden.paused || viewItem == group.hidden.suspended || viewItem == group.visible.unknown || viewItem == group.visible.stopped || viewItem == group.visible.mixed || viewItem == group.visible.running || viewItem == group.visible.paused || viewItem == group.visible.suspended)",
                    "group": "inline@26"
                },
                {
                    "command": "parallels-desktop.tree-view.start-group-vms",
                    "when": "view == parallels-desktop-machines && (viewItem == group.visible.stopped || viewItem == group.visible.mixed || viewItem == group.hidden.stopped || viewItem == group.hidden.mixed)",
                    "group": "inline@1"
                },
                {
                    "command": "parallels-desktop.tree-view.stop-group-vms",
                    "when": "view == parallels-desktop-machines && (viewItem == group.visible.running || viewItem == group.hidden.running )",
                    "group": "inline@2"
                },
                {
                    "command": "parallels-desktop.tree-view.resume-group-vms",
                    "when": "view == parallels-desktop-machines && (viewItem == group.visible.paused || viewItem == group.visible.suspended || viewItem == group.hidden.paused || viewItem == group.hidden.suspended)",
                    "group": "inline@3"
                },
                {
                    "command": "parallels-desktop.tree-view.pause-group-vms",
                    "when": "view == parallels-desktop-machines && (viewItem == group.visible.running || viewItem == group.hidden.running)",
                    "group": "inline@4"
                },
                {
                    "command": "parallels-desktop.tree-view.suspend-group-vms",
                    "when": "view == parallels-desktop-machines && (viewItem == group.visible.running || viewItem == group.hidden.running)",
                    "group": "inline@5"
                },
                {
                    "command": "parallels-desktop.tree-view.take-group-snapshot",
                    "when": "view == parallels-desktop-machines && (viewItem == group.visible.running || viewItem == group.visible.mixed || viewItem == group.visible.stopped || viewItem == group.visible.paused || viewItem == group.visible.suspended || viewItem == group.hidden.running || viewItem == group.hidden.mixed || viewItem == group.hidden.stopped || viewItem == group.hidden.paused || viewItem == group.hidden.suspended)",
                    "group": "inline@6"
                },
                {
                    "command": "parallels-desktop.vagrant.box.provider.init",
                    "when": "view == parallels-desktop-vagrant && viewItem == vagrant.box",
                    "group": "inline"
                },
                {
                    "command": "parallels-desktop.vagrant.box.provider.delete",
                    "when": "view == parallels-desktop-vagrant && viewItem == vagrant.box",
                    "group": "inline"
                }
            ]
        },
        "views": {
            "parallels-desktop": [
                {
                    "id": "parallels-desktop-machines",
                    "name": "Virtual Machines",
                    "icon": "media/parallels_logo.svg",
                    "contextualTitle": "Parallels Desktop Virtual Machines"
                },
                {
                    "when": "parallels-desktop:vagrant == true",
                    "id": "parallels-desktop-vagrant",
                    "name": "Vagrant Boxes",
                    "icon": "media/parallels_logo.svg",
                    "contextualTitle": "Vagrant Boxes"
                }
            ]
        },
        "viewsWelcome": [
            {
                "view": "parallels-desktop-machines",
                "when": "parallels-desktop:initialized == true && parallels-desktop:hasVirtualMachines == false",
                "contents": "Welcome to Parallels Desktop\n Please add a virtual machine to start using the extension.\n[Create Virtual Machine](command:parallels-desktop.tree-view.add-vm)"
            },
            {
                "view": "parallels-desktop-vagrant",
                "when": "parallels-desktop:hasVagrantBoxes === undefined",
                "contents": "Loading Vagrant Boxes..."
            },
            {
                "view": "parallels-desktop-vagrant",
                "when": "parallels-desktop:initialized == true && parallels-desktop:vagrant == true && parallels-desktop:hasVagrantBoxes == false",
                "contents": "Welcome to Vagrant Boxes Management\n Please add a Vagrant box to start using the extension."
            }
        ]
    },
    "scripts": {
        "clean": "rimraf ./dist ./out",
        "vscode:prepublish": "npm run build",
        "compile": "webpack && gulp copy-images",
        "watch": "webpack --watch",
        "build": "webpack --mode production && gulp copy-images",
        "package": "npm run clean && vsce package",
        "open-in-browser": "vscode-test-web --extensionDevelopmentPath=src .",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "lint": "eslint src --ext ts",
        "lint-fix": "eslint . --ext .ts --fix",
        "format": "prettier --write \"**/*.ts\"",
        "format-check": "prettier --check \"**/*.ts\"",
        "test": "node ./out/test/runTest.js"
    },
